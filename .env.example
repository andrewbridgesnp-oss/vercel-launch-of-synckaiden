# Database Connection (Required)
# Provided automatically by Manus platform
DATABASE_URL=mysql://user:password@host:port/database

# OAuth Configuration (Required)
OAUTH_SERVER_URL=https://oauth.manus.space
OWNER_OPEN_ID=your-owner-id

# JWT Secret (Required)
JWT_SECRET=your-secret-key-here

# Application Configuration
VITE_APP_ID=synckaiden
NODE_ENV=development

# Built-in Forge API (Optional)
BUILT_IN_FORGE_API_URL=
BUILT_IN_FORGE_API_KEY=

# Analytics (Optional)
# Umami analytics integration - leave empty to disable
VITE_ANALYTICS_ENDPOINT=
VITE_ANALYTICS_WEBSITE_ID=

# Example with values:
# VITE_ANALYTICS_ENDPOINT=https://analytics.yourdomain.com
# VITE_ANALYTICS_WEBSITE_ID=your-website-id-here

# External API Keys (Optional - see API_INTEGRATION_GUIDE.md)
# Stripe
STRIPE_SECRET_KEY=
STRIPE_PUBLISHABLE_KEY=
STRIPE_WEBHOOK_SECRET=

# PayPal
PAYPAL_CLIENT_ID=
PAYPAL_CLIENT_SECRET=
PAYPAL_WEBHOOK_ID=

# OpenAI (Optional - Manus provides built-in LLM)
OPENAI_API_KEY=

# AWS S3 (Optional)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=
S3_BUCKET=

# Google APIs (Optional)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# Printful (Optional)
PRINTFUL_API_KEY=

# Shopify (Optional)
SHOPIFY_API_KEY=
SHOPIFY_API_SECRET=

# Rate Limiting Configuration (Optional)
RATE_LIMIT_WINDOW=60000
